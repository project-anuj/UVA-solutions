#include<bits/stdc++.h>
using namespace std;
#define REP(i,n) for(int i=0;i<n;i++)
int main()
{
	int n;
	int j=1;
	while(true)
	{	int zero=0,nonzero=0;
		cin>>n;
		if(n==0)
		break;
		int a[n];
		REP(i,n)
		cin>>a[i];
		REP(i,n)
		{
			if(a[i]==0)
			zero++;
			else
			nonzero++;
		}
		cout<<"Case "<<j<<": "<<nonzero-zero<<endl;
		j++;
	}
}
